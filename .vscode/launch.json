{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cli",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/lib/cli.js",
      "args": [
        "-k",
        "package.json"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "test",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/test/v8.js",
      "args": [
        "--native-parser"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "fail",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/benchmarks/fail",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
